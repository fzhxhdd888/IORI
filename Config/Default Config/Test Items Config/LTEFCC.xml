<xml>
	<TestItems>
			<item  Name="Init seting" index="0">
				<parms>
					<Parm>
						<Name>Band</Name>
						<Value>FDD-LTE 2</Value>
						<Type>1</Type>
						<Range>FDD-LTE 2,FDD-LTE 4,FDD-LTE 5,FDD-LTE 7,FDD-LTE 12,FDD-LTE 17,FDD-LTE 25,FDD-LTE 26,TDD-LTE 38,TDD-LTE 41</Range>
						<info>选择Band。</info>
					</Parm>
					<Parm>
						<Name>BandWidth</Name>
						<Value>1.4MHz</Value>
						<Type>1</Type>
						<Range>1.4MHz,3MHz,5MHz,10MHz,15MHz,20MHz</Range>
						<info>选择BandWidth。</info>
					</Parm>
					<Parm>
						<Name>DL Ref Level</Name>
						<Value>-85</Value>
						<Type>2</Type>
						<Range>-136 to -6</Range>
						<info>BS DL Reference Level</info>
					</Parm>
				</parms>
				<limits>
				</limits>
			</item>	
			<item  Name="Conducted output power" index="1">
				<parms>
					<Parm>
						<Name>Antenna Gain</Name>
						<Value>0</Value>
						<Type>2</Type>
						<Range>-20 to 20</Range>
						<info>天线增益，用于计算ERP/EIRP</info>
					</Parm>
					<Parm>
						<Name>RB</Name>
						<Value>1RB/PRB/FRB</Value>
						<Type>1</Type>
						<Range>1RB/PRB/FRB,1RB,PRB,FRB</Range>
						<info>RB count，Default value:1/PRB/FRB</info>
					</Parm>
					<Parm>
						<Name>Modulation</Name>
						<Value>QPSK/16-QAM</Value>
						<Type>1</Type>
						<Range>QPSK/16-QAM,QPSK,16-QAM</Range>
						<info>modulation type，Default value:QPSK/16-QAM</info>
					</Parm>
				</parms>
				<limits>
					<limit>
						<Name>FDD-LTE 5/26 ERP Limit(dBm)</Name>					
						<low>None</low>
						<high>38.45</high>
						<Type>2</Type>
						<Range></Range>
						<info>FDD-LTE 5/26 ERP Limit, Default value: LOW:None, HIGH:38.45dBm(7W)</info>				
					</limit>
					<limit>
						<Name>FDD-LTE 12/17 ERP Limit(dBm)</Name>					
						<low>None</low>
						<high>34.77</high>
						<Type>2</Type>
						<Range></Range>
						<info>FDD-LTE 12/17 ERP Limit, Default value: LOW:None, HIGH:34.77dBm(3W)</info>				
					</limit>
					<limit>
						<Name>FDD-LTE 2/7/25 TDD-LTE 41 EIRP Limit(dBm)</Name>					
						<low>None</low>
						<high>33</high>
						<Type>2</Type>
						<Range></Range>
						<info>FDD-LTE 2/7/25 TDD-LTE 41 EIRP Limit, Default value: LOW:None, HIGH:33dBm(2W)</info>				
					</limit>
					<limit>
						<Name>FDD-LTE 4 EIRP Limit(dBm)</Name>					
						<low>None</low>
						<high>30</high>
						<Type>2</Type>
						<Range></Range>
						<info>FDD-LTE 4 EIRP Limit, Default value: LOW:None, HIGH:30dBm(1W)</info>				
					</limit>
				</limits>
			</item>	
			<item  Name="Peak-to-Average Ratio" index="2">
				<parms>	
					<Parm>
						<Name>RB</Name>
						<Value>1RB/FRB</Value>
						<Type>1</Type>
						<Range>1RB/FRB,1RB,FRB</Range>
						<info>RB count，Default value:1RB/FRB</info>
					</Parm>
					<Parm>
						<Name>Modulation</Name>
						<Value>QPSK/16-QAM</Value>
						<Type>1</Type>
						<Range>QPSK/16-QAM,QPSK,16-QAM</Range>
						<info>modulation type，Default value:QPSK/16-QAM</info>
					</Parm>
				</parms>
				<limits>
					<limit>
						<Name>Peak-to-Average Ratio Limit(dB)</Name>					
						<low>None</low>
						<high>13</high>
						<Type>2</Type>
						<Range></Range>
						<info>Peak-to-Average Ratio Limit Limit, Default value: LOW:None, HIGH:13dB</info>				
					</limit>
				</limits>
			</item>			
			<item  Name="99% Occupied Bandwidth" index="3">
				<parms>
					<Parm>
						<Name>RB</Name>
						<Value>FRB</Value>
						<Type>1</Type>
						<Range>FRB</Range>
						<info>RB count，Default value:FRB</info>
					</Parm>
					<Parm>
						<Name>Modulation</Name>
						<Value>QPSK/16-QAM</Value>
						<Type>1</Type>
						<Range>QPSK/16-QAM,QPSK,16-QAM</Range>
						<info>modulation type，Default value:QPSK/16-QAM</info>
					</Parm>
				</parms>
				<limits>
					<limit>
						<Name>99% Occupied Bandwidth Limit(MHz)</Name>					
						<low>None</low>
						<high>None</high>
						<Type>2</Type>
						<Range></Range>
						<info>99% Occupied Bandwidth Limit, Default value: LOW:None, HIGH:None</info>				
					</limit>
				</limits>
			</item>
			<item  Name="26dB Occupied Bandwidth" index="4">
				<parms>
					<Parm>
						<Name>RB</Name>
						<Value>FRB</Value>
						<Type>1</Type>
						<Range>FRB</Range>
						<info>RB count，Default value:FRB</info>
					</Parm>
					<Parm>
						<Name>Modulation</Name>
						<Value>QPSK/16-QAM</Value>
						<Type>1</Type>
						<Range>QPSK/16-QAM,QPSK,16-QAM</Range>
						<info>modulation type，Default value:QPSK/16-QAM</info>
					</Parm>
				</parms>
				<limits>
					<limit>
						<Name>26dB Occupied Bandwidth Limit(MHz)</Name>					
						<low>None</low>
						<high>None</high>
						<Type>2</Type>
						<Range></Range>
						<info>26dB Occupied Bandwidth Limit, Default value: LOW:None, HIGH:None</info>				
					</limit>
				</limits>
			</item>	 
			<item  Name="Band edge" index="5">
				<parms>
					<Parm>
						<Name>RB</Name>
						<Value>1RB/FRB</Value>
						<Type>1</Type>
						<Range>1RB/FRB,1RB,FRB</Range>
						<info>RB count，Default value:1RB/FRB</info>
					</Parm>
					<Parm>
						<Name>Modulation</Name>
						<Value>QPSK/16-QAM</Value>
						<Type>1</Type>
						<Range>QPSK/16-QAM,QPSK,16-QAM</Range>
						<info>modulation type，Default value:QPSK/16-QAM</info>
					</Parm>
				</parms>
				<limits>	
				</limits>
			</item>	
			<item  Name="Conducted Spurious emissions" index="6">
				<parms>	
					<Parm>
						<Name>RB</Name>
						<Value>1RB</Value>
						<Type>1</Type>
						<Range>1RB</Range>
						<info>RB count，Default value:1RB</info>
					</Parm>
					<Parm>
						<Name>Modulation</Name>
						<Value>QPSK/16-QAM</Value>
						<Type>1</Type>
						<Range>QPSK/16-QAM,QPSK,16-QAM</Range>
						<info>modulation type，Default value:QPSK/16-QAM</info>
					</Parm>
				</parms>
				<limits>
