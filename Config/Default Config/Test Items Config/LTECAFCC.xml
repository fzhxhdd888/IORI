<xml>
	<TestItems>
			<item  Name="Init seting" index="0">
				<parms>
					<Parm>
						<Name>Band</Name>
						<Value>CA_2C</Value>
						<Type>1</Type>
						<Range>CA_2C,CA_5B,CA_7C,CA_12B,CA_38C,CA_41C</Range>
						<info>选择Band。</info>
					</Parm>
					<Parm>
						<Name>DL Ref Level</Name>
						<Value>-85</Value>
						<Type>2</Type>
						<Range>-136 to -6</Range>
						<info>BS DL Reference Level</info>
					</Parm>
				</parms>
				<limits>
				</limits>
			</item>	
			<item  Name="Conducted Power and EIRP" index="1">
				<parms>
					<Parm>
						<Name>Antenna Gain</Name>
						<Value>0</Value>
						<Type>2</Type>
						<Range>-20 to 20</Range>
						<info>天线增益，用于计算ERP/EIRP</info>
					</Parm>
					<Parm>
						<Name>Modulation</Name>
						<Value>QPSK/16-QAM</Value>
						<Type>1</Type>
						<Range>QPSK/16-QAM/64-QAM,QPSK/16-QAM,QPSK,16-QAM,64QAM</Range>
						<info>modulation type，Default value:QPSK/16-QAM</info>
					</Parm>
				</parms>
				<limits>
					<limit>
						<Name>CA_5B ERP Limit(dBm)</Name>					
						<low>None</low>
						<high>38.45</high>
						<Type>2</Type>
						<Range></Range>
						<info>CA_5B ERP Limit, Default value: LOW:None, HIGH:38.45dBm(7W)</info>				
					</limit>
					<limit>
						<Name>CA_12B ERP Limit(dBm)</Name>					
						<low>None</low>
						<high>34.77</high>
						<Type>2</Type>
						<Range></Range>
						<info>CA_12B ERP Limit, Default value: LOW:None, HIGH:34.77dBm(3W)</info>				
					</limit>
					<limit>
						<Name>CA_2C/7C/38C/41C EIRP Limit(dBm)</Name>					
						<low>None</low>
						<high>33</high>
						<Type>2</Type>
						<Range></Range>
						<info>CA_2C/7C/38C/41C EIRP Limit, Default value: LOW:None, HIGH:33dBm(2W)</info>				
					</limit>
				</limits>
			</item>	
			<item  Name="Peak-to-Average Ratio" index="2">
				<parms>	
					<Parm>
						<Name>Modulation</Name>
						<Value>QPSK/16-QAM</Value>
						<Type>1</Type>
						<Range>QPSK/16-QAM,QPSK,16-QAM</Range>
						<info>modulation type，Default value:QPSK/16-QAM</info>
					</Parm>
				</parms>
				<limits>
					<limit>
						<Name>Peak-to-Average Ratio Limit(dB)</Name>					
						<low>None</low>
						<high>13</high>
						<Type>2</Type>
						<Range></Range>
						<info>Peak-to-Average Ratio Limit Limit, Default value: LOW:None, HIGH:13dB</info>				
					</limit>
				</limits>
			</item>			
			<item  Name="99% Occupied Bandwidth" index="3">
				<parms>
					<Parm>
						<Name>Modulation</Name>
						<Value>QPSK/16-QAM</Value>
						<Type>1</Type>
						<Range>QPSK/16-QAM/64-QAM,QPSK/16-QAM,QPSK,16-QAM,64-QAM</Range>
						<info>modulation type，Default value:QPSK/16-QAM</info>
					</Parm>
				</parms>
				<limits>
					<limit>
						<Name>99% Occupied Bandwidth Limit(MHz)</Name>					
						<low>None</low>
						<high>None</high>
						<Type>2</Type>
						<Range></Range>
						<info>99% Occupied Bandwidth Limit, Default value: LOW:None, HIGH:None</info>				
					</limit>
				</limits>
			</item>
			<item  Name="26dB Occupied Bandwidth" index="4">
				<parms>
					<Parm>
						<Name>Modulation</Name>
						<Value>QPSK/16-QAM</Value>
						<Type>1</Type>
						<Range>QPSK/16-QAM/64-QAM,QPSK/16-QAM,QPSK,16-QAM,64-QAM</Range>
						<info>modulation type，Default value:QPSK/16-QAM</info>
					</Parm>
				</parms>
				<limits>
					<limit>
						<Name>26dB Occupied Bandwidth Limit(MHz)</Name>					
						<low>None</low>
						<high>None</high>
						<Type>2</Type>
						<Range></Range>
						<info>26dB Occupied Bandwidth Limit, Default value: LOW:None, HIGH:None</info>				
					</limit>
				</limits>
			</item>	 
			<item  Name="Band edge" index="5">
				<parms>
					<Parm>
						<Name>Modulation</Name>
						<Value>QPSK/16-QAM</Value>
						<Type>1</Type>
						<Range>QPSK/16-QAM,QPSK,16-QAM</Range>
						<info>modulation type，Default value:QPSK/16-QAM</info>
					</Parm>
				</parms>
				<limits>	
				</limits>
			</item>	
			<item  Name="Out of Band Emissions" index="6">
				<parms>	
					<Parm>
						<Name>Modulation</Name>
						<Value>QPSK/16-QAM</Value>
						<Type>1</Type>
						<Range>QPSK/16-QAM,QPSK,16-QAM</Range>
						<info>modulation type，Default value:QPSK/16-QAM</info>
					</Parm>
				</parms>
				<limits>
				</limits>
			</item>	
			<item  Name="Frequency stability" index="7">
				<parms>	
					<Parm>
						<Name>Modulation</Name>
						<Value>QPSK</Value>
						<Type>1</Type>
						<Range>QPSK</Range>
						<info>modulation type，Default value:QPSK</info>
					</Parm>
				</parms>
				<limits>
				</limits>
			</item>
	</TestItems>
</xml>